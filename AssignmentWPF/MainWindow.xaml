<Window x:Class="AssignmentWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssignmentWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <TextBox x:Name="AgeBox" HorizontalAlignment="Left" Margin="146,142,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" />
        <TextBox x:Name="LocationBox" HorizontalAlignment="Left" Margin="494,142,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Enter Age" HorizontalAlignment="Left" Margin="144,111,0,0" VerticalAlignment="Top"/>
        <Label Content="Location (rural/urban)" HorizontalAlignment="Left" Margin="494,111,0,0" VerticalAlignment="Top"/>
        <Label Content="Amount to pay" HorizontalAlignment="Left" Margin="302,279,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Start" Content="Start Calculation" HorizontalAlignment="Left" Margin="310,196,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBlock x:Name="Result" HorizontalAlignment="Left" Margin="302,323,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
    </Grid>
</Window>
